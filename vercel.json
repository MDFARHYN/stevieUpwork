{
    "version": 2,
    "buildCommand": "cd frontend_remix && npm install && npm run build",
    "outputDirectory": "frontend_remix/public",
    "framework": null,
    "functions": {
      "backend_django/wsgi.py": {
        "runtime": "python3.9"
      }
    },
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/backend_django/wsgi.py"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }